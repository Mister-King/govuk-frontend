{% extends "layout.njk" %}

{% set variants = {
  'Standard link': '',
  'Link with no visited state': 'govuk-link--no-visited-state',
  'Link with normal text colour': 'govuk-link--text-colour',
  'Muted link': 'govuk-link--muted'
} %}

{% set states = {
  'Link': ':link',
  'Visited link': ':visited',
  'Hovered link': ':hover',
  'Active link': ':active',
  'Focused link': ':focus',
  'Focused visited link': ':focus :visited',
  'Focused hovered link': ':focus :hover',
  'Focused active link': ':focus :active'
} %}

{% block beforeContent %}
  {{ govukBackLink({
    "href": "/"
